-- Status Section  
local StatusSection = MainTab:AddSection("Status")

local StatusParagraph = MainTab:AddParagraph({
    Title = "Current Status",
    Content = "Ch∆∞a b·∫Øt ƒë·∫ßu"
})

-- Update status every 3 seconds
task.spawn(function()
    while true do
        local status = "ƒêang ch·ªù..."
        
        if getgenv().AutoMainAccount then
            if HasGodChalice() then
                status = "‚úÖ C√≥ God Chalice - ƒêang summon Rip Indra"
            else
                local elite, eliteCFrame = DetectEliteHunter()
                if elite then
                    status = "‚öîÔ∏è ƒêang farm: " .. elite.Name
                else
                    status = "üîÑ ƒêang t√¨m Elite ho·∫∑c hop server"
                end
            end
        else
            status = "‚ùå Auto ƒë√£ t·∫Øt"
        end
        
        StatusParagraph:SetDesc(status)
        task.wait(3)
    end
end)

